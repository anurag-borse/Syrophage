@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


<!-- 40416-->

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Responsive Multipurpose HTML5 Business Template">
    <meta name="author" content="Shreethemes">

    <link rel="shortcut icon" href="images/favicon.ico">

    <title>Alita - A Responsive Multipurpose HTML Template</title>

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700|Poppins" rel="stylesheet">
    <!-- Bootstrap core CSS -->
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Magnificpopup Css -->
    <link rel="stylesheet" type="text/css" href="~/assets/css/magnific-popup.css">
    <!-- Bootstrap core CSS -->
    <link href="~/assets/css/fontawesome.css" rel="stylesheet" />
    <link href="~/assets/css/materialdesignicons.min.css" rel="stylesheet" />
    <link href="~/assets/css/pe-icon-7-stroke.css" rel="stylesheet" />


    <!--Slider-->
    <link href="~/assets/css/slick.css" rel="stylesheet" />
    <link href="~/assets/css/slick-theme.css" rel="stylesheet" />


    <!-- Custom styles for this template -->
    <link href="~/assets/css/style.css" rel="stylesheet" />
    <link href="~/assets/css/menu.css" rel="stylesheet" />
    <link href="~/assets/css/default.css" rel="stylesheet" />

    <style>
        #error{
            background-image: url('../assets/images/home/404.jpg');
        }
    </style>

</head>

<body>
    <!-- Loader -->
    <div id="preloader">
        <div id="status">
            <div class="spinner">
                <div class="double-bounce1"></div>
                <div class="double-bounce2"></div>
            </div>
        </div>
    </div>

    <!-- ERROR PAGE -->
    <section class="section bg-home" id="error">
        <div class="bg-overlay"></div>
        <div class="home-center">
            <div class="home-desc-center">
                <div class="container">
                    <div class="row">
                        <div class="col-12 text-center error-page">
                            <a href="index-2.html">
                                <img src="~/screenshot_2024-04-26_152838-removebg-preview.png" alt="missing_logo" height="50">
                                <div class="spacer-15"></div>
                            </a>
                            <h4 class="text-white">Error 404: Page's Not Found</h4>
                            <h1 class="text-white">404!</h1>
                            <h4 class="text-white">Please Back Again Later</h4>
                            <p class="text-white mt-3">Use the navigation above or the button below to get back and track.</p>
                            <a asp-action="index" asp-controller="Home" class="btn btn-custom mt-4">Go Back Home</a>
                        </div>
                    </div>
                </div> <!-- end container -->
            </div>
        </div>
    </section>
    <!-- ERROR PAGE -->
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/assets/js/jquery.min.js"></script>
    <script src="~/assets/js/popper.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>

    <!-- Portfolio -->
    <script src="~/assets/js/jquery.magnific-popup.min.js"></script>
    <script src="~/assets/js/isotope.js"></script>
    <script src="js/isotope.js"></script>
    <!-- Carousel -->
    <script src="~/assets/js/slick.min.js"></script>
    <script src="~/assets/js/slick.init.js"></script>

    <!--custom script-->
    <script src="~/assets/js/app.js"></script>

</body>


<!-- 40417-->
</html>